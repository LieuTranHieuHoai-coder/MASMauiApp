<?xml version="1.0" encoding="UTF-8" ?>
<ContentPage
    x:Class="MASMauiApp.XamlPages.AssetInfoPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:d="http://schemas.microsoft.com/dotnet/2021/maui/design"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:MASMauiApp.XamlPages"
    xmlns:localPage="clr-namespace:MASMauiApp"
    xmlns:pages="clr-namespace:MASMauiApp.Components.Pages"
    mc:Ignorable="d">
    <BlazorWebView x:Name="CounterWebView" HostPage="wwwroot/index.html" StartPath="/assetinfo">
        <BlazorWebView.RootComponents>
            <RootComponent Selector="#app" ComponentType="{x:Type localPage:Routes}"/>
        </BlazorWebView.RootComponents>
    </BlazorWebView>
</ContentPage>
